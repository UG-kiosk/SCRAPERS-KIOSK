{
    "name": "scrapers-kiosk",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "start": "NODE_ENV=production node dist/index.js",
        "dev": "nodemon",
        "build": "tsc",
        "test:unit:ci": "jest --testPathPattern=src/utils",
        "test:coverage:ci": "jest --coverage --testPathPattern=tests/",
        "prettier:ci": "npx prettier --list-different .",
        "lint:ci": "eslint . --ext .ts"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "ext": "ts",
        "exec": "ts-node src/index.ts"
    },
    "dependencies": {
        "@types/node": "~20.11.24",
        "axios": "~1.6.7",
        "cheerio": "~1.0.0-rc.12",
        "cors": "~2.8.5",
        "eslint": "~8.57.0",
        "express": "~4.18.3",
        "lodash": "~4.17.21",
        "papaparse": "~5.4.1",
        "typescript": "~5.3.3"
    },
    "devDependencies": {
        "@types/cors": "~2.8.17",
        "@types/express": "~4.17.21",
        "@types/jest": "~29.5.12",
        "@types/lodash": "~4.14.202",
        "@types/papaparse": "~5.3.14",
        "jest": "~29.7.0",
        "nodemon": "^3.1.0",
        "prettier": "~3.2.5",
        "ts-node": "^10.9.2"
    }
}
